{
  "name": "se-tracking-engine",
  "version": "1.0.0",
  "description": "Act as the middleware for the PGSB, PVSB devices and serve the content to the users by consuming and listening to RESTful APIs",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "npm run build && node dist/index.js",
    "clean": "rimraf dist && rimraf logs",
    "lint": "eslint --fix --config .eslintrc.json \"src/**/*.js\"",
    "build": "npm run clean && babel src -s -D -d dist",
    "watch": "nodemon -e js --watch src --exec \"npm run start\""
  },
  "keywords": [
    "Smart Electricity Tracking Engine",
    "SE Tracking Engine"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aravindakr95/se-tracking-engine.git"
  },
  "author": {
    "name": "Aravinda Rathnayake",
    "email": "aravindacloud@gmail.com",
    "url": "https://github.com/aravindakr95"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aravindakr95/se-tracking-engine/issues"
  },
  "homepage": "https://github.com/aravindakr95/se-tracking-engine#readme",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.12",
    "morgan": "^1.10.0",
    "postmark": "^2.6.0",
    "rimraf": "^3.0.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.3",
    "babel-core": "^6.26.3",
    "chalk": "^4.1.0",
    "eslint": "^7.12.1",
    "faker": "^5.1.0",
    "nodemon": "^2.0.6",
    "regenerator-runtime": "^0.13.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "12.16.1"
          }
        }
      ]
    ]
  },
  "engines": {
    "node": "12.16.1"
  }
}
